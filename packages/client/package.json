{
  "name": "@penumbra-zone/client",
  "version": "4.0.1",
  "license": "(MIT OR Apache-2.0)",
  "description": "Package for connecting to any Penumbra extension, including Prax.",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rfv dist package penumbra-zone-client-*.tgz",
    "lint": "eslint \"**/*.ts*\"",
    "prepack": "pnpm clean && pnpm build"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./prax": "./src/prax.ts",
    "./global": "./src/global.ts",
    "./get-port": "./src/get-port.ts"
  },
  "publishConfig": {
    "exports": {
      "./prax": {
        "import": "./dist/prax.js",
        "types": "./dist/prax.d.ts"
      },
      "./global": {
        "import": "./dist/global.js",
        "types": "./dist/global.d.ts"
      }
    }
  },
  "dependencies": {
    "@penumbra-zone/protobuf": "workspace:*",
    "@penumbra-zone/transport-dom": "workspace:*"
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^1.9.0",
    "@connectrpc/connect": "^1.4.0"
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "^1.9.0",
    "@connectrpc/connect": "^1.4.0"
  }
}
