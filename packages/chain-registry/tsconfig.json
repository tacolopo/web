{
  "compilerOptions": {
      "baseUrl": ".",
      "rootDir": "src",
      "skipLibCheck": true,
      "emitDeclarationOnly": false,
      "declaration": true,
      "esModuleInterop": true,
      "target": "es2022",
      "module": "es2022",
      "isolatedModules": true,
      "allowJs": true,
      "downlevelIteration": true,
      "moduleResolution": "node",
      "resolveJsonModule": true
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}